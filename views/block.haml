%div
  = partial :block_header, locals: {:block => @block}

  %ul.pagination
    - (1..@max_pages).each do |page|
      %li{:class => ('active' if page.to_i == @page.to_i)}
        %a(href="/blocks/#{@block['height']}?page=#{(page).to_s}")
          =page


  - @txes.each do |tx|
    - @tx = tx
    = partial :transaction, locals: {:block => @block, :tx => @tx}
